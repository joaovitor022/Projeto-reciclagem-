CREATE TABLE USUARIOCOMUM (
    IDCONTAUSUARIO INT AUTO_INCREMENT PRIMARY KEY,
    IDUSUARIO VARCHAR(500),
    SENHA VARCHAR(500),
    NOMEUSUARIO VARCHAR(500),
    SOBRENOME VARCHAR(500),
    CPF VARCHAR(500),
    ENDERECO VARCHAR(500),
    DOAR INT,
    ENTREGAR INT,
    RETIRADA INT
);

CREATE TABLE FUNCIONARIO (
    IDCONTAFUNCIONARIO INT AUTO_INCREMENT PRIMARY KEY,
    IDFUNCIONARIO VARCHAR(500),
    SENHA VARCHAR(500),
    ENDERECO VARCHAR(500),
    CNPJ VARCHAR(500),
    NOMEEMPRESA VARCHAR(500)
);

CREATE TABLE PRODUTO (
    IDPRODUTO INT AUTO_INCREMENT PRIMARY KEY,
    PESOPRODUTO INT,
    DESCRICAOPRODUTO VARCHAR(5000),
    MODELOPRODUTO VARCHAR(1000),
    MATERIALPRODUTO VARCHAR(1000),
    QUANTIDADEPRODUTO INT
);
